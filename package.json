{
  "name": "unplugin-react-inspector",
  "version": "0.0.6",
  "description": "A plugin to improve the happiness index of react development",
  "keywords": [
    "unplugin",
    "webpack",
    "vite",
    "rspack",
    "rollup",
    "esbuild",
    "react",
    "react-inspector"
  ],
  "scripts": {
    "play:next": "pnpm run build && pnpm run --filter ./playground/next dev",
    "play:webpack": "pnpm run build && pnpm run --filter ./playground/webpack dev",
    "play:vite": "pnpm run build && pnpm run --filter ./playground/vite dev",
    "play:rspack": "pnpm run build && pnpm run --filter ./playground/rspack dev",
    "release": "pnpm run build && pnpm publish --no-git-checks",
    "dev": "node scripts/prepare.js && rollup -c -w",
    "build": "node scripts/prepare.js && rollup -c && tsc -p ./tsconfig.json",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "prepare": "husky install",
    "commit": "cz",
    "test": "jest"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./client": {
      "import": "./dist/client/index.mjs",
      "require": "./dist/client/index.cjs",
      "types": "./dist/types/client/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "author": "Quincy Qiu",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@jest/globals": "^29.4.3",
    "@qiuqfang/eslint-config": "^0.0.7",
    "@qiuqfang/rollup-plugin-swc": "^0.0.9",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/babel__core": "^7.20.0",
    "@types/connect": "^3.4.35",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.1.2",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.36.0",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "lint-staged": "^13.1.2",
    "postcss": "^8.4.27",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.15.0",
    "rollup-plugin-postcss": "^4.0.2",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.0",
    "@babel/core": "^7.21.8",
    "@babel/plugin-transform-typescript": "^7.21.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "src/**/*.*": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "connect": "^3.7.0",
    "@qiuqfang/get-port": "^1.0.0",
    "launch-editor-middleware": "^2.6.0",
    "magic-string": "^0.30.0",
    "unplugin": "^1.3.1"
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiuqfang/unplugin-react-inspector.git"
  },
  "bugs": {
    "url": "https://github.com/qiuqfang/unplugin-react-inspector/issues"
  },
  "homepage": "https://github.com/qiuqfang/unplugin-react-inspector#readme"
}
